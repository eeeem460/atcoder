{
    "files.associations": {
        "iostream": "cpp",
        "vector": "cpp",
        "ostream": "cpp",
        "iomanip": "cpp",
        "cmath": "cpp",
        "all": "cpp",
        "iosfwd": "cpp",
        "random": "cpp",
        "*.tcc": "cpp"
    },


    // "editor.suggest.snippetsPreventQuickSuggestions": false,


    "editor.snippetSuggestions": "top",

    "center-editor-window.offset": 12,


    "generateSnippet.snippetFilePath": "/mnt/c/Users/user01/AppData/Roaming/Code/User/snippets/",
    // ↓ ワークスペース内にスニペットを作る
    // "generateSnippet.snippetFilePath": "/home/user01/atcoder/.vscode/",


    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        // 自分で実行したいコマンドを設定する
        // $dir, $fileNameはそれぞれ編集中のファイルのディレクトリ、ファイル名を表す
        "cpp": "cd $dir && g++-9 -O0 -std=gnu++17 -Wall -Wextra -Wno-sign-compare -g -fsanitize=undefined,address -I ~/atcoder/ac-library -D MY_DEBUG $fileName -o ~/atcoder/a.out && cd ~/atcoder && ./a.out",
    },
    "code-runner.customCommand": "cd $dir && g++-9 -O0 -std=gnu++17 -Wall -Wextra -Wno-sign-compare -g -fsanitize=undefined,address -I ~/atcoder/ac-library -D MY_DEBUG $fileName -o ~/atcoder/a.out && cd ~/atcoder",
    "code-runner.saveFileBeforeRun": true,
    "code-runner.preserveFocus": false,
    "code-runner.ignoreSelection": true,


    //"files.insertFinalNewline": true

 
    "editor.formatOnSave": true, // .vscode にある .clang-format ファイルを atcoder へ
    "json.format.enable": false,
    "files.autoSave": "afterDelay",
    "editor.minimap.enabled": false,
}